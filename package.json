{
  "name": "jenni",
  "version": "0.1.2",
  "description": "CLI tool to interact with Jenkins server",
  "bin": {
    "jen": "./bin/jen.js"
  },
  "engines": {
    "node": ">=8.11"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint '**/*.js'",
    "lint-fix": "eslint --fix",
    "prettier": "prettier --config ./prettier.config.js --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint-fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m-sureshraj/jenni.git"
  },
  "keywords": [
    "cli-app",
    "cli",
    "jenkins-cli",
    "nodejs"
  ],
  "author": "Sureshraj <m.s.suresh100@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "cli-table": "^0.3.1",
    "commander": "^2.19.0",
    "conf": "^3.0.0",
    "figures": "^2.0.0",
    "got": "^9.6.0",
    "kleur": "^3.0.2",
    "open": "^6.0.0",
    "ora": "^3.2.0",
    "prompts": "^2.0.3",
    "terminal-link": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "1.16.4"
  }
}
